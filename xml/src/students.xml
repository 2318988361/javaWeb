<?xml version="1.0" encoding="utf-8" ?>
<!--
    1.xml类型文件，版本1.0，文件编码：utf-8
    2.students ：root元素，程序员自己写
    3.student表示一个students的子元素，可以有多个
    4.id就是属性，name,age,gender是student元素的子元素
-->
<students>
    <student id="100">
        <name>jack</name>
        <age>18</age>
        <gender>男</gender>
    </student>
    <student id="101">
        <name>marry</name>
        <age>19</age>
        <gender>女</gender>
    </student>
</students>